{
	"name": "@backtix-service/admin-panel",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "ts-node -r dotenv/config ./build",
		"dev": "vite dev",
		"dev:host": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest --run",
		"test:unit:watch": "vitest",
		"start:noenv": "HOST=0.0.0.0 PORT=3001 ts-node ./build"
	},
	"dependencies": {
		"dotenv": "^16.3.1",
		"redis": "^4.6.7",
		"@prisma/client": "^5.7.1"
	},
	"devDependencies": {
		"@backtix-service/config": "^1.0.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^2.1.2",
		"@sveltejs/kit": "^2.4.3",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.30.0",
		"flowbite": "^2.2.1",
		"flowbite-svelte": "^0.44.21",
		"flowbite-svelte-icons": "^0.4.5",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.13",
		"vitest": "^1.0.0",
		"prisma": "^5.7.1"
	},
	"type": "module",
	"description": "Everything you need to build a Svelte project, powered by [`create-svelte`](https://github.com/sveltejs/kit/tree/main/packages/create-svelte).",
	"main": "svelte.config.js",
	"directories": {
		"test": "tests"
	},
